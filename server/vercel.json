{
	"version": 2,
	"builds": [
		{
			"src": "server.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"routes": [{ "src": "/(.*)", "dest": "/" }],
			"dest": "server.js",
			"methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
			"headers": {
				"Access-Control-Allow-Origin": "https://decora-ecommerce-client.vercel.app"
			}
		}
	]
}
